<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.web.*?>
<?import org.eclipse.fx.ui.controls.tabpane.DndTabPane?>


<!--

        Note : This file cannot be editied in gluon due to the DndTabPanes.


        The three 'DndTabPane's may be changed to 'TabPane' in order to edit this
        visually, but they must be replaced once you're done.

-->

<VBox fx:id="root" prefHeight="1108.0" prefWidth="1606.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jdngray77.htmldesigner.frontend.MainViewController">
    <children>
        <BorderPane fx:id="mainBorderPane" VBox.vgrow="ALWAYS">
            <center>
                <SplitPane dividerPositions="0.1745635910224439, 0.8341645885286783" focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" BorderPane.alignment="CENTER">
                    <items>
                        <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0">
                            <items>
                                <VBox fx:id="anchorDockLeftTop" minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                                    <children>
                                        <DndTabPane VBox.vgrow="ALWAYS" tabDragPolicy="REORDER" fx:id="dockLeftTop" side="LEFT" tabClosingPolicy="UNAVAILABLE" tabMaxHeight="500.0" tabMaxWidth="500.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                    </children>
                                </VBox>
                                <VBox fx:id="anchorDockLeftBottom" minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
                                    <children>
                                        <DndTabPane VBox.vgrow="ALWAYS" tabDragPolicy="REORDER" fx:id="dockLeftBottom" prefHeight="200.0" prefWidth="200.0" side="LEFT" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                    </children>
                                </VBox>
                            </items>
                        </SplitPane>
                        <SplitPane dividerPositions="0.7" orientation="VERTICAL">
                            <items>

                                <AnchorPane prefHeight="200.0" prefWidth="200.0">
                                    <children>
                                        <TabPane fx:id="dockEditors" layoutX="77.0" layoutY="395.0" tabClosingPolicy="ALL_TABS" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                    </children>
                                </AnchorPane>
                                <AnchorPane fx:id="dockbottom" prefHeight="200.0" prefWidth="200.0">
                                    <children>
                                        <TabPane  tabDragPolicy="REORDER" fx:id="dockBottom" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                            <tabs>
                                                <Tab text="Documentation">
                                                    <content>
                                                        <WebView fx:id="documentation" prefHeight="200.0" prefWidth="200.0" />
                                                    </content>
                                                </Tab>
                                                <Tab text="Content Editor (Experimental)">
                                                    <content>
                                                        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                                                            <children>
                                                                <HTMLEditor fx:id="htmlEditor" htmlText="&lt;html&gt;&lt;head&gt;&lt;/head&gt;&lt;body contenteditable=&quot;true&quot;&gt;&lt;/body&gt;&lt;/html&gt;" prefHeight="200.0" prefWidth="506.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
                                                            </children>
                                                        </AnchorPane>
                                                    </content>
                                                </Tab>

                                            </tabs>
                                        </TabPane>
                                    </children>
                                </AnchorPane>
                            </items>
                        </SplitPane>
                        <VBox  fx:id="anchorDockRight">
                            <children>
                                <DndTabPane VBox.vgrow="ALWAYS" tabDragPolicy="REORDER" fx:id="dockRight" prefHeight="200.0" prefWidth="200.0" side="RIGHT" tabClosingPolicy="UNAVAILABLE" />
                            </children>
                        </VBox>
                    </items>
                </SplitPane>
            </center>
        </BorderPane>
        <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" style="-fx-background-color: #373737;">
            <children>
                <Label fx:id="lblLeftStatus" onMouseClicked="#showLastStatus" maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="" HBox.hgrow="ALWAYS">
                    <font>
                        <Font size="11.0" />
                    </font>
                    <textFill>
                        <Color red="0.625" green="0.625" blue="0.625" />
                    </textFill>
                </Label>
                <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
                <Label fx:id="lblRightStatus" maxWidth="-1.0" text="" HBox.hgrow="NEVER" />
            </children>
            <padding>
                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
            </padding>
        </HBox>

    </children>
</VBox>
